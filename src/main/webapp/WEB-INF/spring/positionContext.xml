<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="positionService" class="com.psy.position.service.impl.PositionServiceImpl">
		<property name="positionBiz" ref="positionBiz"/>
	</bean>

	<bean id="positionBiz" class="com.psy.position.biz.impl.PositionBizImpl">
		<property name="positionDao" ref="positionDao"/>
	</bean>
	
	<bean id="positionDao" class="com.psy.position.dao.impl.PositionDaoImpl">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"/>
	</bean>

</beans>
